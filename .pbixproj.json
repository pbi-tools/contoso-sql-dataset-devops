{
  "version": "0.12",
  "created": "2022-07-20T14:56:35.3052812+01:00",
  "deployments": {
    "Contoso": {
      "mode": "Dataset",
      "source": {
        "type": "Folder",
        "path": "./Contoso Dataset"
      },
      "authentication": {
        "type": "ServicePrincipal",
        "tenantId": "pbitools.org",
        "clientId": "708b6947-c640-4a74-9100-e4b3312b7182",
        "clientSecret": "%PBI_CLIENT_SECRET%"
      },
      "options": {
        "import": {
        },
        "refresh": {
          "enabled": true,
          "skipNewDataset": false,
          "type": "Full",
          "objects": {
            "Date": "Automatic",
            "Info": "Full"
          },
          "tracing": {
            "enabled": true,
            "logEvents": {
              "filter": [
                "*|ReadData|*"
              ]
            },
            "summary": {
              "events": [
                "TabularRefresh",
                "Process",
                "ReadData",
                "ExecuteSql"
              ],
              "objectTypes": [
                "Partition"
              ],
              "outPath": "./.artifacts/refresh-summary.csv",
              "console": true
            }
          }
        },
        "dataset": {
          "replaceParameters": true,
          "deployEmbeddedReport": true,
          "gateway": {
            "discoverGateways": true,
            "gatewayId": "564ec649-a76b-4a47-b878-6c00c8b77b6f"
          }
        },
        "sqlScripts": {
          "enabled": true,
          "schema": "{{[SQL Schema]}}",
          "connection": {
            "Server": "{{[SQL Server]}}",
            "Initial Catalog": "{{[SQL Database]}}",
            "User ID": "sql_deployment_principal",
            "Password": "%SQL_PWD%"
          },
          "path": "./Views",
          "htmlReportPath": "./.artifacts/sqlScripts.html"
        }
      },
      "parameters": {
        "[Version]": "1.0.0",
        "[SQL Schema]": "PowerBI--{{ENVIRONMENT}}--Contoso@1.0.0",
        "SCHEMA_BASE_NAME": "PowerBI--{{ENVIRONMENT}}--Contoso",
        "[SQL Server]": "pbitools-demo-01.database.windows.net",
        "[SQL Database]": "Contoso",
        "[Environment]": "{{ENVIRONMENT}}",
        "[PBITOOLS_VERSION]": "{{PBITOOLS_VERSION}}",
        "[GH-Branch]": "%GITHUB_REF_NAME%",
        "[GH-RunId]": "%GITHUB_RUN_ID%",
        "[GH-SHA]": "%GITHUB_SHA%"
      },
      "environments": {
        "Development": {
          "workspace": "Preview-3",
          "refresh": {
            "objects": {}
          },
          "parameters": {}
        }
      }
    }
  }
}